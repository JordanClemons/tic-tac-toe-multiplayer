oms\r\n  if(rooms.length === 0){\r\n    return(\r\n      <div className=\"lobby-body\">\r\n      <div className=\"lobby-container\">\r\n          <h1 className=\"lobby-header\">Available Games</h1>\r\n          <h1 className=\"nogames-noti\">No games open right now.</h1>\r\n          <button className=\"refresh-button\" onClick={() => refreshRooms()}><FontAwesomeIcon icon={faSyncAlt}  size=\"4x\" /></button>\r\n      </div>\r\n    </div>\r\n    )\r\n  }\r\n  return (\r\n    <div className=\"lobby-body\">\r\n      <div className=\"lobby-container\">\r\n          <h1 className=\"lobby-header\">Available Games</h1>\r\n          <button className=\"refresh-button\" onClick={() => refreshRooms()}><FontAwesomeIcon icon={faSyncAlt}  size=\"3x\" /></button>\r\n          <div className=\"rooms\">\r\n              {rooms.map((room) => (\r\n                  <RoomCard owner={room[0]} code={room[1]} setRedirectCode={setRedirectCode}></RoomCard>\r\n              ))}\r\n          </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PlayPage;","C:\\Users\\Jorda\\toe7\\src\\components\\playpage\\Game\\helper.js",["160"],"C:\\Users\\Jorda\\toe7\\src\\components\\lobbypage\\roomcard.js",["161","162"],"C:\\Users\\Jorda\\toe7\\src\\components\\playpage\\Game\\board.js",[],"C:\\Users\\Jorda\\toe7\\src\\components\\playpage\\Game\\square.js",[],{"ruleId":"163","replacedBy":"164"},{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","severity":1,"message":"168","line":2,"column":49,"nodeType":"169","messageId":"170","endLine":2,"endColumn":53},{"ruleId":"167","severity":1,"message":"171","line":2,"column":55,"nodeType":"169","messageId":"170","endLine":2,"endColumn":63},{"ruleId":"167","severity":1,"message":"172","line":2,"column":26,"nodeType":"169","messageId":"170","endLine":2,"endColumn":32},{"ruleId":"167","severity":1,"message":"173","line":2,"column":34,"nodeType":"169","messageId":"170","endLine":2,"endColumn":39},{"ruleId":"167","severity":1,"message":"174","li